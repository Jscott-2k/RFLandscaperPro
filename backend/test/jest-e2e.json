{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "..",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@rflp/shared$": "<rootDir>/../shared/src",
    "^@rflp/shared/(.*)$": "<rootDir>/../shared/src/$1"
  }
}
